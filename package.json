{
  "name": "murmur-ping",
  "version": "1.0.4",
  "description": "Retrieve server information from a Murmur (mumble) server",
  "license": "ISC",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Flupster/murmur-ping.git"
  },
  "bugs": {
    "url": "https://github.com/Flupster/murmur-ping/issues"
  },
  "homepage": "https://github.com/Flupster/murmur-ping#readme",
  "author": "Flupster",
  "keywords": [
    "mumble",
    "murmur",
    "ping"
  ],
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "files": [
    "lib/**/*"
  ],
  "scripts": {
    "build": "tsc",
    "format": "prettier --write \"src/**/*.ts\"",
    "lint": "tslint -p tsconfig.json",
    "prepare": "npm run build",
    "prepublishOnly": "npm test && npm run lint",
    "test": "jest --config jestconfig.json",
    "preversion": "npm run lint",
    "version": "npm run format && git add -A src",
    "postversion": "git push && git push --tags"
  },
  "devDependencies": {
    "@types/jest": "^27.4.1",
    "@types/node": "^17.0.23",
    "jest": "^27.5.1",
    "prettier": "^2.6.2",
    "ts-jest": "^27.1.4",
    "tslint": "^6.1.3",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "^4.6.3"
  }
}
